
  
  <div class="row m-0 pb-4">
    <div class="col-md-12 ">
      <h3 class="entry-title pb-3"><%=heading%></h3>
    </div>
    <% if users.length > 0 %>
      <% users.each do |user|%>
        <%= render partial: "/shared/user_cards", locals:{user: user}%>
      <% end %>
    <% else %>
      <p>None</p>
    <%end%>
  </div> <!-- row -->