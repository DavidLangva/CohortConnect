<div class="container">
  <h3 class="entry-title pb-3"><%=heading%></h3>
  <div class="row m-0">
    <% if users.length > 0 %>
      <% users.each do |user|%>
        <%= render partial: "/shared/user_cards", locals:{user: user}%>
      <% end %>
    <% else %>
      <h3>None</h3>
    <%end%>
  </div> <!-- row -->
</div>
</div>