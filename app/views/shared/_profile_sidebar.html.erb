<div class=" col-4 sidebar">
	<div class="create-header row m-0">
		<div class="clearfix">
			<div class="float-left">
			<h5>MY COHORTS</h5>
		</div>
			<div class="float-right">
			<%= render "/shared/create_cohort_modal" %>
		</div>
		</div>
	</div>
	<ul class="list-group">
 		<% cohorts.each do |cohort| %>
  	<li class="list-group-item">
  		<span style="float: left;"><%= cohort.name%></span>
  		<span  style="float: right;"><%= cohort.start_date%></span>
  	</li>
  	<% end%>
	</ul>
</div>


